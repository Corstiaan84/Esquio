<Project>
  <PropertyGroup Label="SDK Versions">
    <NetStandardTargetVersion>netstandard2.0</NetStandardTargetVersion>
    <NetStandardTargetVersion21>netstandard2.1</NetStandardTargetVersion21>
    <NetCoreTargetVersion>netcoreapp3.0</NetCoreTargetVersion>
    <NetTestSdk>16.2.0</NetTestSdk>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

  <PropertyGroup Label="Package information">
    <PackageLicenseUrl>https://github.com/Xabaril/Esquio/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/xabaril/Esquio</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xabaril/Esquio</RepositoryUrl>
    <Authors>Xabaril Contributors</Authors>
    <Company>Xabaril</Company>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>true</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup Label="Esquio Versions">
    <esquio>1.0.0$(VersionSuffix)</esquio>
    <esquioaspnetcore>1.0.0$(VersionSuffix)</esquioaspnetcore>
    <esquioconfigurationstore>1.0.0$(VersionSuffix)</esquioconfigurationstore>
    <esquioentityframeworkstore>1.0.0$(VersionSuffix)</esquioentityframeworkstore>
  </PropertyGroup>
  
  <PropertyGroup Label="Package Dependencies">
    <microsoftcsharp>4.5.0</microsoftcsharp>

    <microsoftaspnetcoremvcnewtonsoftjson>3.0.0-preview8.19405.7</microsoftaspnetcoremvcnewtonsoftjson>
    <microsoftaspnetcoreauthenticationjwtbearer>3.0.0-preview8.19405.7</microsoftaspnetcoreauthenticationjwtbearer>
    
    <microsoftextensionsdependencyinjectionabstractions>3.0.0-preview8.19405.4</microsoftextensionsdependencyinjectionabstractions>
    <microsoftextensionsprimitives>3.0.0-preview8.19405.4</microsoftextensionsprimitives>
    <microsoftextensionsoptions>3.0.0-preview8.19405.4</microsoftextensionsoptions>
    <microsoftextensionsconfigurationabstractions>3.0.0-preview8.19405.4</microsoftextensionsconfigurationabstractions> 
    <microsoftextensionsconfigurationbinder>3.0.0-preview8.19405.4</microsoftextensionsconfigurationbinder>
    <microsoftextensionsconfigurationjson>3.0.0-preview8.19405.4</microsoftextensionsconfigurationjson>
    <microsoftextensionslogging>3.0.0-preview8.19405.4</microsoftextensionslogging>
    <microsoftextensionsloggingabstractions>3.0.0-preview8.19405.4</microsoftextensionsloggingabstractions>
    <microsoftextensionsloggingconsole>3.0.0-preview8.19405.4</microsoftextensionsloggingconsole>

    <microsoftentityframeworkcore>3.0.0-preview8.19405.11</microsoftentityframeworkcore>
    <microsoftentityframeworkcorerelational>3.0.0-preview8.19405.11</microsoftentityframeworkcorerelational>
    <microsoftentityframeworkcoresqlserver>3.0.0-preview8.19405.11</microsoftentityframeworkcoresqlserver>
    <microsoftentityframeworkcoredesign>3.0.0-preview8.19405.11</microsoftentityframeworkcoredesign>

    <serilogextensionslogging>2.0.4</serilogextensionslogging>
    <serilogsettingsconfiguration>3.1.0</serilogsettingsconfiguration>
    <serilogsinkscoloredconsole>3.0.1</serilogsinkscoloredconsole>

    <hellangmiddlewareproblemdetails>3.1.0</hellangmiddlewareproblemdetails>
    <mediatrextensionsmicrosoftdependencyinjection>7.0.0</mediatrextensionsmicrosoftdependencyinjection>
    <fluentvalidationaspnetcore>8.5.0-preview4</fluentvalidationaspnetcore>
  </PropertyGroup>

  <PropertyGroup Label="Testing Dependencies">
    <microsoftextensionshosting>3.0.0-preview8.19405.4</microsoftextensionshosting>
    <microsoftaspnetcoretesthost>3.0.0-preview8.19405.7</microsoftaspnetcoretesthost>
    
    <microsoftentityframeworkcoreinmemory>3.0.0-preview8.19405.11</microsoftentityframeworkcoreinmemory>
    <microsoftentityframeworkcoresqlite>3.0.0-preview8.19405.11</microsoftentityframeworkcoresqlite>
    <npgsqlentityframeworkcorepostgresql>3.0.0-preview8</npgsqlentityframeworkcorepostgresql>
    
    <xunit>2.4.1</xunit>
    <xunitrunner>2.4.1</xunitrunner>
    <newtonsoftjson>12.0.2</newtonsoftjson>
    <fluentassertions>5.7.0</fluentassertions>
    <achevetesthost>2.2.1</achevetesthost>
    <respawn>3.2.0</respawn>
  </PropertyGroup>
  
  <PropertyGroup Label="CLI Tools Versions">
    <DotnetXUnitPackageVersion>2.4.0</DotnetXUnitPackageVersion>
    <DotnetWatcherToolPackageVersion>2.0.0</DotnetWatcherToolPackageVersion>
  </PropertyGroup>
</Project>