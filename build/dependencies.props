<Project>
  <PropertyGroup Label="SDK Versions">
    <NetStandardTargetVersion21>netstandard2.1</NetStandardTargetVersion21>
    <NetCoreTargetVersion>netcoreapp3.0</NetCoreTargetVersion>
    <NetTestSdk>16.3.0</NetTestSdk>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

  <PropertyGroup Label="Package information">
    <PackageLicenseUrl>https://github.com/Xabaril/Esquio/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/xabaril/Esquio</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xabaril/Esquio</RepositoryUrl>
    <Authors>Xabaril Contributors</Authors>
    <Company>Xabaril</Company>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>true</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup Label="Esquio Versions">
    <esquio>1.1.0$(VersionSuffix)</esquio>
    <esquioaspnetcore>1.1.0$(VersionSuffix)</esquioaspnetcore>
    <esquioaspnetcoreapplicationinsightsprocessor>1.1.0$(VersionSuffix)</esquioaspnetcoreapplicationinsightsprocessor>
    <esquioconfigurationstore>1.1.0$(VersionSuffix)</esquioconfigurationstore>
    <esquioentityframeworkstore>1.1.0$(VersionSuffix)</esquioentityframeworkstore>
    <esquiominiprofiler>1.1.0$(VersionSuffix)</esquiominiprofiler>
  </PropertyGroup>

  <PropertyGroup Label="Esquio Contrib Dependencies">
    <EsquioLocationToggles>1.1.1-preview.6</EsquioLocationToggles>
    <EsquioUserAgentToggles>1.1.1-preview.6</EsquioUserAgentToggles>
</PropertyGroup> 
  <PropertyGroup Label="Package Dependencies">
    <microsoftcsharp>4.6.0 </microsoftcsharp>
    <systemdiagnosticsdiagnosticsource>4.6.0</systemdiagnosticsdiagnosticsource>

    <microsoftaspnetcoreauthenticationjwtbearer>3.0.0</microsoftaspnetcoreauthenticationjwtbearer>
    
    <microsoftextensionsdependencyinjectionabstractions>3.0.0</microsoftextensionsdependencyinjectionabstractions>
    <microsoftextensionsprimitives>3.0.0</microsoftextensionsprimitives>
    <microsoftextensionsoptions>3.0.0</microsoftextensionsoptions>
    <microsoftextensionsconfigurationabstractions>3.0.0</microsoftextensionsconfigurationabstractions> 
    <microsoftextensionsconfigurationbinder>3.0.0</microsoftextensionsconfigurationbinder>
    <microsoftextensionsconfigurationjson>3.0.0</microsoftextensionsconfigurationjson>
    <microsoftextensionslogging>3.0.0</microsoftextensionslogging>
    <microsoftextensionsloggingabstractions>3.0.0</microsoftextensionsloggingabstractions>
    <microsoftextensionsloggingconsole>3.0.0</microsoftextensionsloggingconsole>

    <microsoftentityframeworkcore>3.0.0</microsoftentityframeworkcore>
    <microsoftentityframeworkcorerelational>3.0.0</microsoftentityframeworkcorerelational>
    <microsoftentityframeworkcoresqlserver>3.0.0</microsoftentityframeworkcoresqlserver>
    <microsoftentityframeworkcoredesign>3.0.0</microsoftentityframeworkcoredesign>

    <serilogextensionslogging>3.0.1</serilogextensionslogging>
    <serilogsettingsconfiguration>3.1.0</serilogsettingsconfiguration>
    <serilogsinkscoloredconsole>3.0.1</serilogsinkscoloredconsole>

    <hellangmiddlewareproblemdetails>3.1.0</hellangmiddlewareproblemdetails>
    <mediatrextensionsmicrosoftdependencyinjection>7.0.0</mediatrextensionsmicrosoftdependencyinjection>
    <fluentvalidationaspnetcore>8.5.0</fluentvalidationaspnetcore>
    <microsoftapplicationinsights>2.10.0</microsoftapplicationinsights>
    <microsoftapplicationinsightsaspnetcore>2.7.1</microsoftapplicationinsightsaspnetcore>
  </PropertyGroup>

  <PropertyGroup Label="Tools Dependencies">
    <miniprofilershared>4.1.0</miniprofilershared>
    <miniprofiler>4.1.0</miniprofiler>
    <miniprofileraspnetcoremvc>4.1.0</miniprofileraspnetcoremvc>
  </PropertyGroup>

  <PropertyGroup Label="Testing Dependencies">
    <microsoftextensionshosting>3.0.0</microsoftextensionshosting>
    <microsoftaspnetcoretesthost>3.0.0</microsoftaspnetcoretesthost>
    
    <microsoftentityframeworkcoreinmemory>3.0.0</microsoftentityframeworkcoreinmemory>
    <microsoftentityframeworkcoresqlite>3.0.0</microsoftentityframeworkcoresqlite>
    
    <xunit>2.4.1</xunit>
    <xunitrunner>2.4.1</xunitrunner>
    <fluentassertions>5.9.0</fluentassertions>
    <achevetesthost>2.2.1</achevetesthost>
    <respawn>3.2.0</respawn>
  </PropertyGroup>
  
  <PropertyGroup Label="CLI Tools Versions">
    <DotnetXUnitPackageVersion>2.4.0</DotnetXUnitPackageVersion>
    <DotnetWatcherToolPackageVersion>2.0.0</DotnetWatcherToolPackageVersion>
  </PropertyGroup>
</Project>