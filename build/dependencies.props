<Project>
  <PropertyGroup Label="SDK Versions">
    <NetStandardTargetVersion>netstandard2.0</NetStandardTargetVersion>
    <NetStandardTargetVersion21>netstandard2.1</NetStandardTargetVersion21>
    <NetCoreTargetVersion>netcoreapp3.0</NetCoreTargetVersion>
    <NetTestSdk>16.2.0</NetTestSdk>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

  <PropertyGroup Label="Package information">
    <PackageLicenseUrl>https://github.com/Xabaril/Esquio/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/xabaril/Esquio</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xabaril/Esquio</RepositoryUrl>
    <Authors>Xabaril Contributors</Authors>
    <Company>Xabaril</Company>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>true</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup Label="Esquio Versions">
    <esquio>1.0.0$(VersionSuffix)</esquio>
    <esquioaspnetcore>1.0.0$(VersionSuffix)</esquioaspnetcore>
    <esquioaspnetcoreapplicationinsightsprocessor>1.0.0$(VersionSuffix)</esquioaspnetcoreapplicationinsightsprocessor>
