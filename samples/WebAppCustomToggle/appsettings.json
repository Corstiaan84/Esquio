{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Esquio": {
    "Applications": [
      {
        "Name": "WebApp",
        "Features": [
          {
            "Name": "FeaturedContent",
            "Enabled": "true",
            "Toggles": [
              {
                "Type": "WebAppCustomToggle.Toggles.MyCustomToggle, WebAppCustomToggle"
              }
            ]
          },
          {
            "Name": "Privacy",
            "Enabled": "true",
            "Toggles": [
              {
                "Type": "Esquio.Toggles.OffToggle"
              }
            ]
          }
        ]
      }
    ]
  }
}
