{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Esquio": {
    "Applications": [
      {
        "Name": "WebApp",
        "Features": [
          {
            "Name": "MatchScore",
            "Enabled": "true",
            "Toggles": [
              {
                "Type": "Esquio.Toggles.OnToggle"
              }
            ]
          },
          {
            "Name": "Privacy",
            "Enabled": "true",
            "Toggles": [
              {
                "Type": "Esquio.Toggles.OffToggle"
              }
            ]
          },
          {
            "Name": "BetaUser",
            "Enabled": "true",
            "Toggles": [
              {
                "Type": "Esquio.Toggles.UserNameToggle",
                "Parameters": [
                  {
                    "Name": "Users",
                    "Value": "betauser;beta"
                  }
                ]
              }
            ]
          },

          {
            "Name": "AlphaUser",
            "Enabled": "true",
            "Toggles": [
              {
                "Type": "Esquio.Toggles.UserNameToggle",
                "Parameters": [
                  {
                    "Name": "Users",
                    "Value": "alphauser;alpha"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}
