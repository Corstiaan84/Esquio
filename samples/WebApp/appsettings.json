{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Esquio": {
    "Products": [
      {
        "Name": "WebApp",
        "Features": [
          {
            "Name": "MatchScore",
            "Enabled": true,
            "Toggles": [
              {
                "Type": "Esquio.Toggles.OnToggle"
              }
            ]
          },
          {
            "Name": "DarkMode",
            "Enabled": true,
            "Toggles": [
              {
                "Type": "Esquio.Toggles.OffToggle"
              }
            ]
          },
          {
            "Name": "AnimationsMatch",
            "Enabled": true,
            "Toggles": [
              {
                "Type": "Esquio.Toggles.OnToggle"
              }
            ]
          },
          {
            "Name": "BuyTickets",
            "Enabled": true,
            "Toggles": [
              {
                "Type": "WebApp.Toggles.LocationToggle, WebApp",
                "Parameters": {
                  "Countries": "France;Spain"
                }
              }
            ]
          },
          {
            "Name": "MinutesProgressBar",
            "Enabled": true,
            "Toggles": [
              {
                "Type": "Esquio.Toggles.UserNameToggle",
                "Parameters": {
                  "Users": "betauser;beta"
                }
              }
            ]
          },
          {
            "Name": "MinutesRealTime",
            "Enabled": true,
            "Toggles": [
              {
                "Type": "Esquio.Toggles.UserNameToggle",
                "Parameters": {
                  "Users": "alphauser;alpha"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
