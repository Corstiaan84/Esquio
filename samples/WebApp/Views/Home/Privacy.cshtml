@inject IFeatureService featureService
@{
    ViewData["Title"] = "Privacy Policy";

    if(featureService.IsEnabledAsync(Flags.NavigationSection).Result)
    {
        ViewData["FeatureStatus"] = "IsActive";
    }
    else
    {
        ViewData["FeatureStatus"] = "IsNotActive";
    }
}

<h1>@ViewData["Title"]</h1>



<p>The feature NavigationSection status is @ViewData["FeatureStatus"]</p>
