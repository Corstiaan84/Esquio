{
  "name": "app-vue",
  "description": "Stable version of a basic SPA example with SCSS, Vue and TypeScript.",
  "license": "MIT",
  "version": "3.0.0",
  "scripts": {
    "build": "vue-cli-service build --dest \"../wwwroot\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "vue-cli-service lint",
    "lint:styles": "stylelint \"./src/**/*.(scss|vue)\"",
    "dev": "vue-cli-service build --watch --dest \"../wwwroot\"",
    "dotnet": "dotnet run -p ..",
    "infrastructure": "docker-compose -f \"../../../build/docker-compose-infrastructure.yml\" up",
    "start": "run-p infrastructure dev dotnet"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "bootstrap": "^4.3.1",
    "bootstrap-material-design": "^4.1.2",
    "bootstrap-vue": "^2.0.0-rc.19",
    "core-js": "^2.6.5",
    "fetch-intercept": "^2.3.1",
    "inversify-props": "^1.0.2",
    "jquery": "^3.4.1",
    "localforage": "^1.7.3",
    "normalize-scss": "^7.0.1",
    "nprogress": "^0.2.0",
    "popper.js": "^1.15.0",
    "vee-validate": "^2.2.11",
    "vue": "^2.6.6",
    "vue-i18n": "^8.10.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.1",
    "vue-toasted": "^1.1.27",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-cypress": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^5.0.0",
    "require.all": "^2.0.4",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.5.4",
    "terser-webpack-plugin": "^1.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21"
  }
}
