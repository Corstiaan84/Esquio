<li class="page-item@(CurrentPage == Page - 1 ? " active" : "")">
    <a class="page-link"
       href="#"
       @onclick="OnClick"
       @onclick:preventDefault>
        @Page
    </a>
</li>


@code {
    [Parameter] public int Page { get; set; }
    [Parameter] public int CurrentPage { get; set; }
    [Parameter] public EventCallback OnClick { get; set; }
}
